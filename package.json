{
  "name": "my-react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "./node_modules/.bin/tsc --b && ./node_modules/.bin/eslint src/**/*.{t,j}s{,x} --no-error-on-unmatched-pattern --quiet && vite build",
    "preview": "vite preview",
    "test": "./node_modules/.bin/jest",
    "test:watch": "./node_modules/.bin/jest --watch",
    "test:coverage": "./node_modules/.bin/jest --coverage",
    "prettier": "./node_modules/.bin/tsc --noEmit && ./node_modules/.bin/prettier src/**/*.{t,j}s{,x} --no-error-on-unmatched-pattern --check",
    "lint": "./node_modules/.bin/tsc --noEmit && ./node_modules/.bin/eslint src/**/*.{t,j}s{,x} --no-error-on-unmatched-pattern --no-warn-ignored",
    "lint:fix": "npm run lint -- --fix",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "prepare": "git config core.autocrlf false && npx husky install",
    "pre-commit": "lint-staged --allow-empty"
  },
  "engines": {
    "node": ">=22.x"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.10.0",
    "@mui/icons-material": "^6.4.0",
    "@mui/material": "^6.4.0",
    "@mui/styled-engine-sc": "^6.4.0",
    "@mui/system": "^6.4.0",
    "@reduxjs/toolkit": "^2.5.0",
    "@testing-library/jest-dom": "^6.6.3",
    "axios": "^1.7.9",
    "axios-mock-adapter": "^2.1.0",
    "dompurify": "^3.2.4",
    "i18next": "^24.2.1",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.1",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-html-parser": "^2.0.2",
    "react-i18next": "^15.4.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.1.1",
    "react-router-dom": "^7.1.1",
    "styled-components": "^6.1.14",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "@rollup/plugin-alias": "^5.1.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^29.7.0",
    "commit-packages": "file:./configs/commit-packages",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-and-format-packages": "file:./configs/lint-and-format-packages",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "terser": "^5.37.0",
    "typescript": "^5.7.3",
    "unplugin-auto-import": "^19.0.0",
    "vite": "^6.0.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "style",
          "test",
          "fix",
          "docs",
          "release",
          "update",
          "perf",
          "chore"
        ]
      ]
    }
  },
  "prettier": "./configs/prettier.config.cjs",
  "config": {
    "commitizen": {
      "path": "cz-git",
      "czConfig": "./configs/cz.config.cjs"
    }
  }
}
